<% title "Photo" %>

<p></p>
<%= render @photo %>

<%= link_to "Comments", photo_comments_path(@photo), class:"btn btn-outline-primary" %>
<%= link_to "Edit", edit_photo_path(@photo), class:"btn btn-outline-primary" %> 
<%= link_to "Delete", @photo, method: :delete, data: {confirm: "Are you sure"}, class:"btn btn-outline-danger" %> 
<%= link_to "All Photos", photos_path, class:"btn btn-outline-dark" %>