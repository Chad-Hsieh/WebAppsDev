<% title "Photos" %>

<% @photos.each do |photo| %>
    <div id="task_<%= photo.id %>" class="photo">
        <p><strong>URL:</strong> <%= photo.photo_url %></p>
        <p><strong>Description:</strong> <%= photo.description %></p>
        <p><strong>Hashtag:</strong> <%= photo.hashtag %></p>
    </div>
<% end %>

<%= link_to "New Photo", new_photo_path, class: "btn btn-primary" %>